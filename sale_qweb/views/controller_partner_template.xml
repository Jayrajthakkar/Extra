<?xml version="1.0" encoding="utf-8"?>
<odoo>
<!-- Index -->
<template id="partner_template_page" name="Partners">
    <t t-call="website.layout">
        <t t-set="additional_title">Partners</t>
        <h1 class='text-center mt-2 mb-2'>Partner Detail Form</h1>
		<div id="wrap">
            <div class="container" id="container">
                <form action="/create/partner" method="post" enctype="multipart/form-data">
                    <div class="form-group">
                        <label for="inputname">Name</label>
                        <input type="text" class="form-control placeholder" id="inputname" name="name" placeholder="Enter Name"/>
                      </div>
                      <div class="form-group">
                        <label for="inputemail">Email address</label>
                        <input type="email" class="form-control" id="inputemail" name="email" aria-describedby="emailHelp" placeholder="Enter email"/>
                        <small id="emailHelp" class="form-text">We'll never share your email with anyone else.</small>
                      </div>
                     <div class="form-group">
                        <label for="inputphone">Phone Number</label>
                       <input type="phone" class="form-control" id="inputphone" placeholder="Enter Phone Number" name="phone"/>
                      </div>
                     <div class="form-group">
                        <label for="inputfile">Upload Files</label>
                        <input type="file" id="inputfile" class="form-control" name="image_1920"/>
                      </div>
                      
                      <button type="submit"  class="submitbtn" >Submit</button>
                      <br></br>
                </form>
            </div>
        </div>
            
           
    </t>
</template>

</odoo>
